source ~/.bash_profile
수정

혹은

eval "$(pyenv init -)"

//
파일 합치고 중복 제거
for file in data/*
do
    cat "$file" >> dnsFull
done
sort dnsFull > dnsSorted
uniq dnsSorted dnsOut
rm dnsFull dnsSorted